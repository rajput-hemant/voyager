    "eslint-plugin-react-compiler": "^0.0.0",
    "lucide-react": "^0.399.0",
    "next": "^15.0.0-rc.0",
    "next-themes": "^0.3.0",
    "react": "^19.0.0-rc-fb9a90fa48-20240614",
